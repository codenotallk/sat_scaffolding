#include <application.h>
#include <string.h>

sat_status_t %s_init (%s_t *object)
{
    sat_status_t status = sat_status_set (&status, false, "%s init error");

    if (object != NULL)
    {

        sat_status_set (&status, true, "");        
    }

    return status;
}

sat_status_t %s_open (%s_t *object, %s_args_t *args)
{
    sat_status_t status = sat_status_set (&status, false, "%s open error");

    if (object != NULL && args != NULL)
    {

        sat_status_set (&status, true, "");        
    }

    return status;
}

sat_status_t %s_run (%s_t *object)
{
    sat_status_t status = sat_status_set (&status, false, "%s run error");

    if (object != NULL)
    {

        sat_status_set (&status, true, "");        
    }

    return status;
}

sat_status_t %s_close (%s_t *object)
{
    sat_status_t status = sat_status_set (&status, false, "%s close error");

    if (object != NULL)
    {

        sat_status_set (&status, true, "");
    }

    return status;
}